    "@langchain/langgraph": "^0.2.16",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.5",
    "langchain": "^0.3.33",
    "sqlite3": "^5.1.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.15"
  }
